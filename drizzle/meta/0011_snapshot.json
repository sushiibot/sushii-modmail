        "thread_id_check": {
          "name": "thread_id_check",
          "value": "\"messages\".\"thread_id\" NOT GLOB '*[^0-9]*'"
        },
        "message_id_check": {
          "name": "message_id_check",
          "value": "\"messages\".\"message_id\" NOT GLOB '*[^0-9]*'"
